{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug",
      "stopOnEntry": false,
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["debug"],
      "cwd": "${workspaceFolder}",
      "outputCapture": "std",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "NODE_ENV": "development",
        "TEST_ERROR": "_start"
      }
    }
  ]
}
